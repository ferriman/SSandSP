void calculateMatrix() {
  for (int x=0;x<matrixSizeX;x++) {
    for (int y=0;y<matrixSizeY;y++) {
      //println("alpha",x,y,screenMap[x][y][4]);
      if (screenMap[x][y][0]==1) {
        if (screenMap[x][y][4]<=0) {
          if (int(random(20))==1) {
            screenMap[x][y][1]=int(random(255));
            screenMap[x][y][2]=int(random(255));
            screenMap[x][y][3]=int(random(255));
            screenMap[x][y][4]=1;
            //println("created",x,y);
          }
        } 
        if (screenMap[x][y][4] > 0 && screenMap[x][y][5]==1) {
          screenMap[x][y][4]=screenMap[x][y][4]+5;
          if (screenMap[x][y][4]>=255) { screenMap[x][y][5]=0; }
        }
        if (screenMap[x][y][4] > 0 && screenMap[x][y][5]==0) {
            screenMap[x][y][4]=screenMap[x][y][4]-5;
            if (screenMap[x][y][4]<=0) { screenMap[x][y][5]=1; }
        }
      }
    }
  }
}


void setup() {
  size(1920,1080);
   for (int x=0;x<matrixSizeX;x++) {
    for (int y=0;y<matrixSizeY;y++) {
        if (hex(city[(y*70)+x]).equals("FF000000")) { screenMap[x][y][0]=1; println("FET");} else { screenMap[x][y][0]=0; }
      screenMap[x][y][1]=0;
      screenMap[x][y][2]=0;
      screenMap[x][y][3]=0;
      screenMap[x][y][4]=0;
      screenMap[x][y][5]=1;
    }
  }
}

void draw() {
  background(0);
  calculateMatrix();
  for (int x=0;x<matrixSizeX;x++) {
    for (int y=0;y<matrixSizeY;y++) {
      fill(screenMap[x][y][1],screenMap[x][y][2],screenMap[x][y][3],screenMap[x][y][4]);
      //fill(255,0,0);
      rect(x*(sizeX+padding), y*(sizeX+padding), sizeX, sizeX);
    }
  }
}
